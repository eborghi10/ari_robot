<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <!-- Diff drive controller multipliers -->
  <rosparam command="load" file="$(find ari_controller_configuration)/config/mobile_base_controller_multipliers.yaml"/>

  <!-- Diff drive controller parameters -->
  <rosparam command="load" file="$(find ari_controller_configuration)/config/mobile_base_controller.yaml"/>

  <!-- Robot model -->
  <include file="$(find ari_description)/robots/upload.launch">
    <!--arg name="camera_model"  value="$(arg camera_model)"/-->
  </include>

  <!-- ari_hardware parameters -->
  <rosparam command="load" file="$(find ari_bringup)/config/ari_hardware/ari_hardware_no-arm.yaml"/>

  <!-- Node that forwards commands and feedback of dynamixel motors -->
  <include file="$(find ari_bringup)/launch/dynamixel_node.launch"/>

  <!-- Bring up robot -->
  <include file="$(find ari_bringup)/launch/ari_bringup.launch">
    <!--arg name="camera_model"  value="$(arg camera_model)"/-->
  </include>
</launch>
